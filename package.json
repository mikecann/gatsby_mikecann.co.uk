{
  "name": "hexo-site",
  "version": "0.0.0",
  "private": true,
  "hexo": {
    "version": "3.3.9"
  },
  "scripts": {
    "postinstall": "cd themes/tranquilpeak && yarn install",
    "deploy": "yarn clean && yarn generate && yarn upload && yarn invalidate && yarn update-algolia",
    "generate": "hexo generate && yarn copy-admin-config",
    "copy-admin-config": "copyfiles ./source/admin/config.yml ./public/admin -u 2",
    "upload": " aws s3 sync public s3://mikecann-web --delete",
    "invalidate": "aws cloudfront create-invalidation --distribution-id E3Q14GS9N3OYRM --paths /index.html /atom.xml",
    "dev": "hexo server -p 4500",
    "migrate": "hexo migrate wordpress migration/mikecanncouk.wordpress.2017-10-27-ORIGINAL.xml",
    "update-algolia": "rimraf db.json && hexo algolia",
    "clean": "rimraf public"
  },
  "dependencies": {
    "copyfiles": "^1.2.0",
    "hexo": "^3.2.0",
    "hexo-algoliasearch": "^0.2.1",
    "hexo-deployer-s3-cloudfront": "^0.1.4",
    "hexo-generator-archive": "^0.1.4",
    "hexo-generator-category": "^0.1.3",
    "hexo-generator-feed": "^1.2.2",
    "hexo-generator-index": "^0.2.0",
    "hexo-generator-tag": "^0.2.0",
    "hexo-migrator-image": "^1.0.5",
    "hexo-migrator-wordpress": "file:./hexo-migrator-wordpress",
    "hexo-renderer-ejs": "^0.3.0",
    "hexo-renderer-marked": "^0.3.0",
    "hexo-renderer-stylus": "^0.3.1",
    "hexo-server": "^0.2.2",
    "rimraf": "^2.6.2"
  },
  "devDependencies": {
    "shelljs": "^0.7.8"
  }
}
